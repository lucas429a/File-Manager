
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model ETIQUETAS{
  id String @id @default("autogenerated")
  TipoEtiqueta String?
  QTD String?
  N_PEDIDO String?
  CODIGO_MASTER String?
  CODIGO_DIGITO String?
  FORNECEDOR String?
  COR String?
  DESCRICAO String?
  UC String?
  GRADE String?
  GRADE_QTD String?
  TOTAL String?
  SEMANA String?
  NOME_EMPRESA String?
  VOLUME String?
  ANO String?
  PROMOCAO String?
  DEPARTAMENTO String?
  N_TAMANHO String?
  EAN String?
  CODIGO_MATERIAL_SKU String?
  PRECO_VDA String?
  FAIXA String?
  ITEM String?
  SUBSEGMENTO String?
  CICLOVIDA String?
  QUANTIDADE_PECA String?
  CENTRO_ENTREGA String?
  CENTRO_FATURAMENTO String?
  COD_BARRA_CD_ATUAL String?
  COD_BARRA String?
  DES_ENDERECO String?
  DES_AUXILIAR_1 String?
  DES_AUXILIAR_2 String?
  COD_DCO String?
  DES_CODIGO_DESTINO String?
  QTD_TOTAL String?
  EMPRESA_ID Int 
  EMPRESA Empresa @relation(fields: [EMPRESA_ID], references: [ID])
}

model Empresa {
  ID Int @id
  NOME String
  APELIDO String 
  ETIQUETAS ETIQUETAS[] 
  ETIQUETAS_DINAMICAS ETIQUETAS_DINAMICAS[]
}

model ETIQUETAS_DINAMICAS {
  id            String   @id @default(uuid())
  NOME          String
  EMPRESA_ID    Int
  EMPRESA       Empresa  @relation(fields: [EMPRESA_ID], references: [ID])
  TEMPLATE      String
  COMPLETE_OPTIONS Json
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}